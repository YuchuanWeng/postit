<%= render 'shared/errors', obj: @post %>



<%= form_for @post do |f| %>
  <div class="control-group well">
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_area :description, row: 3, class: "span4" %>
  </div>

  <%= f.submit "Create Post", class: 'btn btn-primary' %>
<% end %>
